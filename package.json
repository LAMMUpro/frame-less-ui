{
  "name": "frame-less-ui",
  "version": "0.0.0",
  "description": "frame-less-ui, 一个跨框架的轻量前端ui组件库",
  "author": "lammu",
  "private": true,
  "keywords": [
    "frame-less-ui",
    "framelessui",
    "ui library",
    "web component",
    "vue",
    "shadow dom"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "cross-env output=js yarn vite build",
    "build:vue2": "yarn vite build --config ./vite.config.vue2.ts",
    "build:all": "yarn build & yarn build:vue2",
    "new:comp": "ts-node scripts/newVueComponent.ts",
    "docs": "cd ./docs && yarn dev"
  },
  "dependencies": {
    "qrcode": "^1.5.3",
    "vue": "3.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@types/node": "^20.11.20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.3.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-legacy": "4.1.1",
    "@vitejs/plugin-vue2": "^2.3.1",
    "vue2fless": "0.7.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "~1.59.0",
    "typescript": "^5.3.3",
    "vite": "^4.3.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "3.9.1",
    "@rollup/plugin-terser": "0.4.4"
  },
  "workspaces": {
    "packages": [
      "docs",
      "dependences/vitejs-plugin-vue2fless"
    ],
    "nohoist": [
      "docs/*"
    ]
  },
  "resolutions": {

  },
  "engines": {
    "node": ">=16.0.0"
  }
}
