{
  "name": "frame-less-ui",
  "version": "1.0.9-alpha.7",
  "description": "frame-less-ui, 一个跨框架的轻量前端ui组件库",
  "author": "lammu",
  "keywords": [
    "frame-less-ui",
    "framelessui",
    "ui library",
    "web component",
    "vue",
    "shadow dom"
  ],
  "type": "module",
  "types": "./types/index.d.ts",
  "exports": {
    "./global.d.ts": {
      "types": "./types/src/global.d.ts"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./esm/index.js"
    },
    "./*": {
      "types": "./types/components/*.d.ts",
      "default": "./esm/components/*.js"
    }
  },
  "dependencies": {
    "qrcode": "^1.5.3",
    "vue3fless": "0.5.13"
  }
}
