{
  "name": "frame-less-ui",
  "version": "1.0.9-alpha.10",
  "description": "frame-less-ui, 一个跨框架的轻量前端ui组件库",
  "author": "lammu",
  "keywords": [
    "frame-less-ui",
    "framelessui",
    "ui library",
    "web component",
    "vue",
    "shadow dom"
  ],
  "type": "module",
  "types": "./types/index.d.ts",
  "exports": {
    "./global.d.ts": {
      "types": "./types/src/global.d.ts"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./esm/index.js"
    },
    "./*": {
      "types": "./types/components/*/index.d.ts",
      "default": "./esm/components/*/index.js"
    },
    "./vue3/*": {
      "types": "./types/components/*/index.vue3.vue.d.ts",
      "default": "./esm/components/*/vue3.js"
    },
    "./react/*": {
      "types": "./types/components/*/index.d.ts",
      "default": "./esm/components/*/react.js"
    },
    "./vue2/*": {
      "types": "./types/components/*/index.d.ts",
      "default": "./esm/components/*/vue2.js"
    }
  },
  "dependencies": {
    "qrcode": "^1.5.3",
    "vue3fless": "0.5.13",
    "vue2fless": "0.7.16"
  }
}
